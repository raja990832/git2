pipeline {
   agent any

   stages {
      stage('Hello') {
         steps {
            echo 'Hello World'
         }
      }
      
      stage('Job') {
         steps {
            echo 'Build success'
         }
      }
      
      stage('Jenkinsfile Job') {
         steps {
            echo 'Jenkinsfile Build success'
         }
      }
   }
}
